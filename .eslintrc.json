{
    "env": {
        "node": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "warn",
        "no-console": "warn",
        "object-shorthand": "warn"
    }
}
